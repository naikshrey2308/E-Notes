{
    "name": "E-Notes",
    "version": "0.0.0.1",
    "manifest_version": 2,
    "description": "Increase your productivity by keeping notes inside the browser itself.",
    "icons": {
        "16": "./icons/notes-icon16x16.png",
        "32": "./icons/notes-icon32x32.png",
        "48": "./icons/notes-icon48x48.png",
        "128": "./icons/notes-icon128x128.png"
    },
    "browser_action": {
        "default_popup": "./popup/popup.html"
    },
    "permissions": [
        "storage"
    ]
}